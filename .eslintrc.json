{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "globals": {
      "graphql": false
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "object-curly-newline": 0,
      "react/jsx-filename-extension": false,
      "react/forbid-prop-types": false,
      "import/no-extraneous-dependencies": false,
      "linebreak-style": "off",
      "arrow-body-style": 0,
      "no-console": 0,
    }
}